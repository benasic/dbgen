<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.BinaryGeneratorController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Binary type:">
            <font>
                <Font size="16.0" />
            </font></Label>
        <Label fx:id="typeLabel" layoutX="120.0" layoutY="14.0" text="unknown" textFill="#ffad08">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
      <TextField fx:id="numberOfRandomBytesTextField" layoutX="206.0" layoutY="45.0" prefHeight="25.0" prefWidth="137.0" />
      <RadioButton fx:id="randomBytesRadioButton" layoutX="28.0" layoutY="49.0" mnemonicParsing="false" text="Number of random bytes:">
         <toggleGroup>
            <ToggleGroup fx:id="binaryToggleGroup" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="imageRadioButton" layoutX="29.0" layoutY="89.0" mnemonicParsing="false" text="Use images:" toggleGroup="$binaryToggleGroup" />
      <Button fx:id="imagePathButton" layoutX="206.0" layoutY="85.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="137.0" text="Choose images ..." />

    </children>
</AnchorPane>
