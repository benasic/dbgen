<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.DateGeneratorController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Date type:">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Label layoutX="16.0" layoutY="58.0" text="Start date:" />
      <DatePicker fx:id="startDatePicker" layoutX="95.0" layoutY="54.0" />
      <Label fx:id="dateTypeLabel" layoutX="95.0" layoutY="14.0" text="unknown" textFill="#ffad08">
         <font>
            <Font size="16.0" />
         </font></Label>
      <DatePicker fx:id="endDatePicker" layoutX="95.0" layoutY="94.0" />
      <Label layoutX="18.0" layoutY="98.0" text="End date:" />
      <Label layoutX="298.0" layoutY="58.0" text="Start time:" />
      <Label layoutX="301.0" layoutY="98.0" text="End time:" />
      <TextField fx:id="startTimeTextField" layoutX="371.0" layoutY="54.0" prefHeight="25.0" prefWidth="75.0" />
      <TextField fx:id="endTimeTextField" layoutX="371.0" layoutY="94.0" prefHeight="25.0" prefWidth="75.0" />
      <HBox fx:id="daysHBox" layoutX="18.0" layoutY="141.0" prefHeight="25.0" prefWidth="429.0" spacing="20.0">
         <children>
            <CheckBox mnemonicParsing="false" text="Mon." fx:id="monCheckBox" />
            <CheckBox mnemonicParsing="false" text="Tue." fx:id="tueCheckBox" />
            <CheckBox mnemonicParsing="false" text="Wed." fx:id="wenCheckBox"/>
            <CheckBox mnemonicParsing="false" text="Thu." fx:id="thuCheckBox"/>
            <CheckBox mnemonicParsing="false" text="Fri." fx:id="friCheckBox"/>
            <CheckBox mnemonicParsing="false" text="Sat." fx:id="satCheckBox"/>
            <CheckBox mnemonicParsing="false" text="Sun." fx:id="sunCheckBox"/>
         </children>
      </HBox>
   </children>
</AnchorPane>
